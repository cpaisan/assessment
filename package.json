{
  "name": "assessment",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@material-ui/core": "^4.1.1",
    "@material-ui/styles": "^4.1.1",
    "axios": "^0.19.0",
    "lodash.debounce": "^4.0.8",
    "material-ui": "^0.20.2",
    "react": "^16.8.6",
    "react-apollo": "^2.5.8",
    "react-dom": "^16.8.6",
    "react-scripts": "3.0.1"
  },
  "scripts": {
    "start": "react-scripts start & npm run server:mock",
    "build": "react-scripts build",
    "test":
      "start-server-and-test start:test http://localhost:3000/ cypress:interactive",
    "eject": "react-scripts eject",
    "start:test":
      "BROWSER=none HTTPS=false REACT_APP_ENVIRONMENT=dev react-scripts start",
    "test":
      "start-server-and-test start:test http://localhost:3000/ cypress:interactive",
    "start:mocking":
      "REACT_APP_MOCKING=on REACT_APP_ENVIRONMENT=dev react-scripts start",
    "cypress:interactive": "TZ=UTC cypress open",
    "server:mock": "json-server --watch --port 4000 mock_api/db.json"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [">0.2%", "not dead", "not op_mini all"],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "cypress": "^3.3.1",
    "json-server": "^0.15.0",
    "start-server-and-test": "^1.9.1"
  }
}
